{
    "name": "kblais/laravel-uuid",
    "description": "A simple library to use UUIDs as your Eloquent model's primary key.",
    "require": {
        "php": ">=5.4",
        "ramsey/uuid": "^3.4",
        "illuminate/database": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4|^4.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Kblais\\Uuid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kblais\\Uuid\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Killian Blais",
            "email": "contact@killianblais.fr"
        }
    ]
}
